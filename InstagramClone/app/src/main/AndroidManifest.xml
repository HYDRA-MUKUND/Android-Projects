<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hydra.instagramclone">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InstagramClone">
        <activity android:name=".Loginusingotppp"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".FirstTimeMobile"
            android:screenOrientation="portrait"/>
        <activity android:name=".HelloStarrMain"
            android:screenOrientation="portrait"/>
        <activity android:name=".DateofbirthReg"
            android:screenOrientation="portrait"/>
        <activity android:name=".Registrationofuser"
            android:screenOrientation="portrait"/>
        <activity android:name=".PhoneOtpVerification"
            android:screenOrientation="portrait"/>
        <activity android:name=".LoginAct"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>